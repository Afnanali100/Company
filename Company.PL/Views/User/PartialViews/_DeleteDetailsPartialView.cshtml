@model UserViewModel


<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.Id"></label>
    <input class="form-control" asp-for="@Model.Id" readonly />
</div>

<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.FName"></label>
    <input class="form-control" asp-for="@Model.FName" readonly />
</div>

<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.LName"></label>
    <input class="form-control" asp-for="@Model.LName" readonly />
</div>

<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.Email"></label>
    <input class="form-control" asp-for="@Model.Email" readonly />
</div>


<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.PhoneNumber"></label>
    <input class="form-control" asp-for="@Model.PhoneNumber" readonly />
</div>