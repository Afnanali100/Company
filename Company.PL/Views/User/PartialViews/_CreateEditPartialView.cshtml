@model UserViewModel



<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.FName"></label>
    <input class="form-control" asp-for="@Model.FName" />
    <span class="text-danger" asp-validation-for="@Model.FName"></span>
</div>

<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.LName"></label>
    <input class="form-control" asp-for="@Model.LName" />
    <span class="text-danger" asp-validation-for="@Model.LName"></span>
</div>


<div class=" form-group">
    <label class="col-form-label" asp-for="@Model.PhoneNumber"></label>
    <input class="form-control" asp-for="@Model.PhoneNumber" />
    <span class="text-danger" asp-validation-for="@Model.PhoneNumber"></span>
</div>
